{
  "root": "apps/rss-parser",
  "sourceRoot": "apps/rss-parser/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/rss-parser",
        "main": "apps/rss-parser/src/main.ts",
        "tsConfig": "apps/rss-parser/tsconfig.app.json",
        "assets": ["apps/rss-parser/src/assets","apps/rss-parser/src/.env"]
      },
      "configurations": {
        "production": {
          "externalDependencies": "none",
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/rss-parser/src/environments/environment.ts",
              "with": "apps/rss-parser/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "rss-parser:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/rss-parser/**/*.ts"]
      }
    }
  },
  "tags": []
}
